<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="45.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxdialog.FXController">
   <center>
      <HBox prefHeight="23.0" prefWidth="320.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#show" text="show" />
            <Button mnemonicParsing="false" onAction="#showAndWait" text="showAndWait" />
            <Button mnemonicParsing="false" onAction="#dialog" text="dialog" />
         </children>
      </HBox>
   </center>
   <bottom>
      <Label fx:id="label" text="ラベル" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
